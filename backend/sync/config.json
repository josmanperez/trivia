{
    "type": "flexible",
    "state": "enabled",
    "development_mode_enabled": false,
    "service_name": "mongodb-atlas",
    "client_max_offline_days": 30,
    "is_recovery_mode_disabled": false,
    "permissions": {
        "rules": {
            "User": [
                {
                    "name": "owner-read-write",
                    "applyWhen": {},
                    "read": {
                        "_id": "%%user.id"
                    },
                    "write": {
                        "_id": "%%user.id"
                    }
                }
            ]
        },
        "defaultRoles": [
            {
                "name": "read-only",
                "applyWhen": {},
                "read": true,
                "write": false
            }
        ]
    },
    "queryable_fields_names": [
        "category_id"
    ]
}
